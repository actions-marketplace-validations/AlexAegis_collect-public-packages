{
	"name": "github-action-collect-public-packages",
	"description": "Collects every package name in a repository that is explicitly marked as private: false",
	"version": "1.0.0",
	"license": "MIT",
	"private": true,
	"keywords": [
		"github-action",
		"managed-by-autotool"
	],
	"author": {
		"email": "alexaegis@gmail.com",
		"name": "Alex Aegis",
		"url": "https://github.com/AlexAegis"
	},
	"repository": {
		"url": "https://github.com/AlexAegis/github-action-collect-public-packages",
		"type": "git"
	},
	"bugs": {
		"email": "alexaegis@gmail.com",
		"url": "https://github.com/AlexAegis/github-action-collect-public-packages/issues"
	},
	"type": "module",
	"config": {
		"engine-strict": true
	},
	"engines": {
		"node": ">=18.10.0",
		"pnpm": ">=8.0.0"
	},
	"workspaces": [
		"apps/*",
		"libs/*",
		"packages/*"
	],
	"scripts": {
		"build": "turbo run build-lib_ build-app_ --concurrency 16 --cache-dir .cache/turbo",
		"build-app": "turbo run build-app_ --concurrency 16 --cache-dir .cache/turbo",
		"build-lib": "turbo run build-lib_ --concurrency 16 --cache-dir .cache/turbo",
		"lint": "turbo run lint_ --concurrency 16 --cache-dir .cache/turbo",
		"lint:depcheck": "turbo run lint:depcheck_ --concurrency 16 --cache-dir .cache/turbo",
		"lint:depcheck_": "depcheck",
		"lint:es": "turbo run lint:es_ --concurrency 16 --cache-dir .cache/turbo",
		"lint:format": "turbo run lint:format_ --concurrency 16 --cache-dir .cache/turbo",
		"lint:format_": "prettier --no-plugin-search --cache-location .cache/prettier --ignore-path .config/workspace-only.prettierignore --check .",
		"lint:md": "turbo run lint:md_ --concurrency 16 --cache-dir .cache/turbo",
		"lint:md_": "remark --frail --no-stdout --silently-ignore --ignore-pattern packages --ignore-pattern libs --ignore-pattern apps .",
		"lint:style": "turbo run lint:style_ --concurrency 16 --cache-dir .cache/turbo",
		"lint:style:css": "turbo run lint:style:css_ --concurrency 16 --cache-dir .cache/turbo",
		"lint:style:html": "turbo run lint:style:html_ --concurrency 16 --cache-dir .cache/turbo",
		"lint:style:scss": "turbo run lint:style:scss_ --concurrency 16 --cache-dir .cache/turbo",
		"lint:svelte": "turbo run lint:svelte_ --concurrency 16 --cache-dir .cache/turbo",
		"lint:tsc": "turbo run lint:tsc_ --concurrency 16 --cache-dir .cache/turbo",
		"publint": "turbo run publint_ --concurrency 16 --cache-dir .cache/turbo",
		"all": "turbo run all_ --concurrency 16 --cache-dir .cache/turbo",
		"format": "turbo run format_ --concurrency 16 --cache-dir .cache/turbo",
		"format_": "prettier --no-plugin-search --cache-location .cache/prettier --ignore-path .config/workspace-only.prettierignore --write .",
		"ncu": "ncu --configFilePath ./.config/ && pnpm up",
		"nuke": "nuke",
		"postinstall": "husky install && autotool",
		"release": "commit-and-tag-version --sign --commit-all",
		"release:major": "commit-and-tag-version --sign --commit-all --release-as major",
		"release:minor": "commit-and-tag-version --sign --commit-all --release-as minor",
		"release:patch": "commit-and-tag-version --sign --commit-all --release-as patch",
		"test": "turbo run test_ --concurrency 16 --cache-dir .cache/turbo && merge-workspace-lcov-reports && lcov-viewer lcov -o ./coverage ./coverage/lcov.info",
		"typedoc": "turbo run typedoc_ --concurrency 16 --cache-dir .cache/turbo",
		"typedoc_": "typedoc"
	},
	"devDependencies": {
		"@alexaegis/autotool-plugin-commitlint": "^0.3.4",
		"@alexaegis/autotool-plugin-depcheck": "^0.3.4",
		"@alexaegis/autotool-plugin-editorconfig": "^0.3.4",
		"@alexaegis/autotool-plugin-eslint": "^0.3.4",
		"@alexaegis/autotool-plugin-git": "^0.3.4",
		"@alexaegis/autotool-plugin-github": "^0.3.4",
		"@alexaegis/autotool-plugin-husky": "^0.3.4",
		"@alexaegis/autotool-plugin-ncu": "^0.3.4",
		"@alexaegis/autotool-plugin-prettier": "^0.3.4",
		"@alexaegis/autotool-plugin-publint": "^0.3.4",
		"@alexaegis/autotool-plugin-remark": "^0.3.4",
		"@alexaegis/autotool-plugin-standard-version": "^0.3.4",
		"@alexaegis/autotool-plugin-stylelint": "^0.3.4",
		"@alexaegis/autotool-plugin-svelte": "^0.3.4",
		"@alexaegis/autotool-plugin-ts": "^0.3.4",
		"@alexaegis/autotool-plugin-turbo": "^0.3.4",
		"@alexaegis/autotool-plugin-typedoc": "^0.3.4",
		"@alexaegis/autotool-plugin-vite": "^0.3.4",
		"@alexaegis/autotool-plugin-vitest": "^0.3.4",
		"@alexaegis/autotool-plugin-vscode": "^0.3.4",
		"@alexaegis/autotool-plugin-workspace": "^0.3.4",
		"@alexaegis/coverage-tools": "^0.3.2",
		"@alexaegis/eslint-config-core": "^0.3.4",
		"@alexaegis/nuke": "^0.3.4",
		"@alexaegis/stylelint-config": "^0.3.4",
		"@alexaegis/ts": "^0.3.4",
		"@lcov-viewer/cli": "^1.3.0",
		"@types/eslint": "^8.40.2",
		"@types/node": "^20.3.3",
		"@vitest/coverage-v8": "^0.32.4",
		"autotool": "^0.1.1",
		"commit-and-tag-version": "^11.2.2",
		"commitlint": "^17.6.6",
		"depcheck": "^1.4.3",
		"eslint": "^8.44.0",
		"husky": "^8.0.3",
		"lint-staged": "^13.2.3",
		"npm-check-updates": "^16.10.13",
		"prettier": "^2.8.8",
		"remark": "^14.0.3",
		"remark-cli": "^11.0.0",
		"stylelint": "^15.10.0",
		"svelte-check": "^3.4.4",
		"ts-node": "^10.9.1",
		"turbo": "^1.10.7",
		"typedoc": "^0.24.8",
		"typescript": "^5.1.6"
	}
}
